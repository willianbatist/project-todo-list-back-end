{
  "name": "todo-list-api",
  "version": "1.0.0",
  "description": "API with details of a todo list",
  "main": "server.js",
  "scripts": {
    "test": "mocha ./api/tests/**/*$NAME*.test.js --exit",
    "start": "node server.js",
    "dev": "nodemon server.js",
    "create": "npx sequelize db:create",
    "up": "npx sequelize db:migrate && npx sequelize db:seed:all",
    "drop": "npx sequelize db:drop"
  },
  "keywords": [
    "Node.js",
    "Sequelize",
    "Todo List",
    "MySql"
  ],
  "author": "Willian Alves Batista",
  "license": "ISC",
  "devDependencies": {
    "chai-http": "^4.3.0",
    "eslint": "^8.18.0",
    "mocha": "^10.0.0",
    "nodemon": "^2.0.18"
  },
  "dependencies": {
    "chai": "^4.3.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "http-status-codes": "^2.2.0",
    "mysql2": "^2.3.3",
    "sequelize": "^6.21.1",
    "sequelize-cli": "^6.4.1",
    "sinon": "^14.0.0"
  }
}
